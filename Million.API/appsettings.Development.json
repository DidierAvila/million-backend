{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AWS": {
    "Region": "us-east-1",
    "Profile": "default",
    "AccessKey": "YOUR_DEV_AWS_ACCESS_KEY",
    "SecretKey": "YOUR_DEV_AWS_SECRET_KEY",
    "S3": {
      "BucketName": "million-property-images-dev",
      "CloudFrontDomain": "",
      "PublicReadEnabled": true,
      "URLExpiration": 24,
      "MaxFileSize": 10485760,
      "AllowedFileTypes": ["image/jpeg", "image/png", "image/gif", "image/bmp", "image/webp"]
    }
  }
}
